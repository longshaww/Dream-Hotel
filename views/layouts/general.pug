                        li.nav-item
                            a.nav-link(href='/users') Users
                        li.nav-item
                            a.nav-link(href='/rooms') Rooms
                        li.nav-item
                            a.nav-link(href='/customers') Customers
                        li.nav-item(style="position:absolute;right:10%")
                            a(href="/logout")
                                i(class="fas fa-sign-out-alt fa-2x" style="color:white")
                        li.nav-item.dropdown
                            a#navbarDropdownMenuLink.nav-link.dropdown-toggle(href='#', role='button', data-bs-toggle='dropdown', aria-expanded='false') Contact
